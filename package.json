{
  "name": "multireducer",
  "version": "2.0.0",
  "description": "A utility to wrap many copies of a single Redux reducer into a single key-based reducer.",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/multireducer"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/multireducer.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/multireducer.min.js --config webpack.config.production.js",
    "clean": "rimraf dist lib",
    "lint": "eslint src",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build",
    "test": "npm run lint && mocha --compilers js:babel-core/register --recursive --require ./test/setup.js"
  },
  "keywords": [
    "react",
    "reactjs",
    "flux",
    "immutable",
    "redux",
    "redux-immutable",
    "react-redux",
    "multireducer",
    "decorator"
  ],
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikras/multireducer/issues"
  },
  "homepage": "https://github.com/erikras/multireducer",
  "dependencies": {
    "immutable": "^3.7.6",
    "redux-immutable": "^3.0.6"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-syntax-class-properties": "^6.0.14",
    "babel-plugin-transform-class-properties": "^6.0.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.13.1",
    "expect": "^1.12.2",
    "immutable": "^3.7.6",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.3",
    "react": "^0.14.1",
    "react-addons-test-utils": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.6",
    "redux-immutable": "^3.0.6",
    "rifraf": "^2.0.2",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.12"
  },
  "peerDependencies": {
    "react-redux": "^3.0.0 || ^4.0.0",
    "redux": "^3.0.0"
  },
  "npmName": "multireducer",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
